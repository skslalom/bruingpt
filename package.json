{
  "name": "frontend-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build-dev": "tsc -b && vite build --mode dev",
    "build-prod": "tsc -b && vite build --mode prod",
